	"tokenColors": [
		//
    // MARK  DATA TYPE
    //

    // NOTE  STRING
    {
      "name": "#e5c07b",
      "scope": [
        "string",
        "string.template.js",
        "meta.object-literal.key.js" // object key
      ],
      "settings": {
        "foreground": "#278329"
      }
    },
  
    {
      "name": "#c678dd",
      "scope": [
        // number
        "constant.numeric", 
      ],
      "settings": {
        "foreground": "#c678dd"
      }
    },

    {
      "name": "#E4E4E4",
      "scope": [
				"source",
        // computed object key
        "meta.brace.square.js",               
        // Object key value separator
        "punctuation.separator.key-value", 
        // declare function name
        "meta.definition.function",           
        // arrow function sign (ex: =>)
        "storage.type.function.arrow",     
        // User defined
				"meta.definition.variable",
        // template string
				"meta.template.expression.js", 
				// class method
				"meta.method.declaration",
        // class name
        "entity.name.type.class",
        "entity.name.class",
        "entity.name.module",
        "storage.identifier",
        // inherited class
        "entity.other.inherited-class",
        // tag, component attribute name
        "entity.other.attribute-name",
        // lib class/type
        "support.type",
        // Json Property
				"support.dictionary.json",
				"variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },

    {
      "name": "#62BAE4",
      "scope": [
        // function call (ex: ())
        "meta.brace.round",             
        // if, else, ...
        "keyword.control.conditional",  
        // for, while, ...
        "keyword.control.flow",         
        // function call
				"meta.function-call",     
				
        // Variable
        "variable.readwrite",
        "variable.readwrite.other.block",
        // class super function
        "variable.language.super",
        "new.expr"      
      ],
      "settings": {
        "foreground": "#0060ff"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#fbbc05"
      }
		},
		
		{
      "name": "#e06c75",
      "scope": [
        "keyword",
        // Language Variable
				"variable.language",
				// class meta
				"meta.class",
        // Built-in constant
        "constant.language",
        // Tag name
        "entity.name.tag",
        "entity.name.tag.class.js",
        "entity.name.tag.class.jsx",
        "support.class.component.js",
        // number
        "constant.numeric",
        // Template expression JS | TS
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.end.js",
        "punctuation.definition.template-expression.begin.ts",
        "punctuation.definition.template-expression.end.ts",
      ],
      "settings": {
        "foreground": "#eb412d"
      }
    },
    {
      "name": "#56b6c2",
      "scope": [
        // User-defined constant
        "constant.character",
        "constant.other",
        // Storage
        "keyword.control",
        // Tag Class and ID
        "entity.name.tag.class",
        "entity.name.tag.id",
        // Library constant
        "support.constant",
        // StyleSheet Variable
        "variable.css",
        "variable.scss",
        "variable.less",
        "variable.sass",
        // StyleSheet Function
        "function.css",
        "function.scss",
        "function.less",
        "function.sass",
        // JSON String
        "structure.dictionary.property-name.json",
        // module.exports
        "support.type.object.module"
      ],
      "settings": {
        "foreground": "#ea3535"
      }
    },
	]
}